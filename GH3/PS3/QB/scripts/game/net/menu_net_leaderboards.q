	if ($lb_list_type = friends)
		Change \{lb_offset = 0}
	endif
	request_leaderboard <...> <Params>
	Change \{user_control_pill_text_color = [
			0
			0
			0
			255
		]}
	Change \{user_control_pill_color = [
			180
			180
			180
			255
		]}
	add_user_control_helper \{Text = "BACK"
