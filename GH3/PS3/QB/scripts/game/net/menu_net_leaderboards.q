script leaderboard_custom_focus 
	Obj_GetID
	if ScreenElementExists Id = {<ObjID> child = Rank}
		SetScreenElementProps Id = {<ObjID> child = Rank} rgba = ($online_dark_purple)
	endif
	if ScreenElementExists Id = {<ObjID> child = the_text}
