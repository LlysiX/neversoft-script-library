		else
			<part> = <blank_part>
			formatText checksumName = gem_array '%s_song_%p%d' s = <song_prefix> p = <part> d = <difficulty_text_nl> AddToStringLookup = true
		endif
	else
		formatText checksumName = gem_array '%s_song_%p%d' s = <song_prefix> p = <part> d = <difficulty_text_nl> AddToStringLookup = true
	endif
	return gem_array = <gem_array>
endscript

