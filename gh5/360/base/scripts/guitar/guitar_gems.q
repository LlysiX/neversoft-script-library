			getplayerinfo <Player> part
			if checksumequals a = <part> b = vocals
				<vocals> = 1
			endif
		endif
		if (<vocals> = 1)
			formatText \{checksumName = gem_array
				'jamsession_song_vocals'
				AddToStringLookup = true}
