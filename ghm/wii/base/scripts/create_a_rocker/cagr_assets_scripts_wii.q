	else
		<info_index> = <index>
	endif
	edit_graphic_get_array_info_vals index = <info_index>
	if (GotParam change_texture_id)
		<texture_id> = <change_texture_id>
	endif
	if (GotParam change_texture_name)
		<texture_name> = <change_texture_name>
