script ui_init_cagr 
	GetArraySize ($edit_graphic_layer_infos)
	if (<array_Size> <= 0)
		Change edit_graphic_layer_infos = ($default_graphic_layer_info)
	endif
	0x9db2b00d
	unload_gempaks
endscript

script ui_create_cagr 
	if GotParam \{skip_destroy}
		ui_event_remove_params \{param = skip_destroy}
	endif
	LoadPak \{'pak\\cagr\\cagr_full_set.pak'
		Heap = BottomUpHeap}
	LoadPak \{'pak\\cagr\\cagr_bink_set.pak'
		Heap = heap_bink}
	LoadPak \{'pak\\cagr\\cagr_fragment.pak'
		Heap = BottomUpHeap}
	if isngc
		MemPushContext \{TopDownHeap}
	endif
	0xa035885b
	set_home_button_allowed
	if 0x8658e568
		destroy_loading_screen
	endif
	SpawnScriptLater \{edit_graphic_create_pre_menu
		params = {
			from_launch = 1
		}}
endscript

script ui_destroy_cagr 
	if isngc
		MemPopContext
	endif
	UnLoadPak \{'pak\\cagr\\cagr_full_set.pak'
		Heap = BottomUpHeap}
	UnLoadPak \{'pak\\cagr\\cagr_bink_set.pak'
		Heap = heap_bink}
	UnLoadPak \{'pak\\cagr\\cagr_fragment.pak'
		Heap = BottomUpHeap}
	if ScreenElementExists \{id = band_name_text}
		DestroyScreenElement \{id = band_name_text}
	endif
	if ScreenElementExists \{id = cagr_edit_vmenu}
		DestroyScreenElement \{id = cagr_edit_vmenu}
	endif
	if NOT GotParam \{skip_destroy}
		if ScreenElementExists \{id = menu_backdrop_container}
			DestroyScreenElement \{id = menu_backdrop_container}
		endif
	endif
	destroy_generic_menu
	generic_ui_destroy
endscript

script edit_graphic_select_random_logo 
	edit_graphic_clear_all
	GetArraySize ($edit_graphic_premade_graphics)
	if ((<array_Size>) <= 0)
		printf \{'No pre-made graphics available!'}
		return
	endif
	GetRandomValue Name = rand_index a = 0 b = (<array_Size> -1) integer
	random_entry = (($edit_graphic_premade_graphics) [<rand_index>])
	printstruct <random_entry>
	Change edit_graphic_layer_infos = <random_entry>
	0x9b97f817
	return 0x7a0e0385 = <random_entry>
endscript
