script edit_graphic_create_pre_menu \{from_launch = 0}
	if ScreenElementExists \{id = band_name_text}
		DestroyScreenElement \{id = band_name_text}
	endif
	if ScreenElementExists \{id = menu_backdrop_container}
		DestroyScreenElement \{id = menu_backdrop_container}
	endif
	if ScreenElementExists \{id = menu_backdrop}
		DestroyScreenElement \{id = menu_backdrop}
	endif
	if ScreenElementExists \{id = cagr_edit_vmenu}
		DestroyScreenElement \{id = cagr_edit_vmenu}
	endif
	create_menu_backdrop \{texture = main_menu_bg}
	get_band_info
	GetGlobalTags <band_info> param = Name
	make_generic_menu \{vmenu_id = cagr_edit_vmenu
		title = qs(0xf6cdd13f)
		num_icons = 1}
	add_generic_menu_text_item \{text = qs(0x7f77dbe6)
		pad_choose_script = edit_graphic_create_main_menu}
	add_generic_menu_text_item \{text = qs(0xcecd1620)
		pad_choose_script = 0x659ce062
		pad_choose_params = {
			save
		}}
	add_generic_menu_text_item \{text = qs(0x85890ab0)
		pad_choose_script = create_new_generic_popup
		pad_choose_params = {
			popup_type = yes_no_menu
			focus_index = 1
			title = qs(0xe15d5bcd)
			text = qs(0xad8ad98b)
			yes_func = 0x659ce062
			yes_func_params = {
				load
			}
			no_func = destroy_generic_popup
		}}
	add_generic_menu_text_item \{text = qs(0xf6a071a3)
		pad_choose_script = randomize_band_logo}
	add_generic_menu_text_item \{text = qs(0xcad23711)
		pad_choose_script = edit_graphic_clear_all_prompt
		pad_choose_params = {
			refresh
		}}
	menu_finish
	LaunchEvent \{Type = focus
		target = cagr_edit_vmenu}
	if getcurrentcasobject
		ps2_get_musician_context_data Name = <cas_object>
		setcasappearance appearance = ($<bandmember_appearance>)
	else
		setcasappearance \{appearance = {
			}}
	endif
	Change \{cas_current_appearance = {
			cas_band_logo = {
				desc_id = cas_band_logo_id
			}
		}}
	if NOT ScreenElementExists \{id = band_logo_preview_sprite}
