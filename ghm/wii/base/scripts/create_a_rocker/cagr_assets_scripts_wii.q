			Type = SpriteElement
			parent = menu_backdrop_container
			texture = ($cas_band_logo_details.textureasset)
			just = [left center]
			rgba = [255 255 255 250]
			Pos = (700.0, 245.0)
			dims = (350.0, 350.0)
			rot_angle = -1
			z_priority = 5
			id = band_logo_preview_sprite
		}
		CreateScreenElement \{Type = SpriteElement
			parent = band_logo_preview_sprite
			texture = pushead_frame
			just = [
				left
				center
			]
			rgba = [
				255
				255
				255
				250
			]
			Pos = (-70.0, 175.0)
			dims = (512.0, 512.0)
			z_priority = 4
			id = 0xa1a90ba5}
	endif
	0x9b97f817
endscript
screen_element_heap = heap_frontend

script edit_graphic_create_main_menu 
	if isngc
		Change \{screen_element_heap = TopDownHeap}
	else
		Change \{screen_element_heap = heap_frontend}
	endif
	if ScreenElementExists \{id = cagr_edit_vmenu}
		DestroyScreenElement \{id = cagr_edit_vmenu}
	endif
	make_generic_menu \{vmenu_id = cagr_edit_vmenu
		title = qs(0xd899144b)
		num_icons = 1
		show_history
		pad_back_script = edit_graphic_exit_main_menu}
	<layer_num> = 0
	begin
	if NOT (($edit_graphic_layer_infos [<layer_num>].texture_name) = qs(0x00000000))
		<layer_num> = (<layer_num> + 1)
	else
		break
	endif
	repeat ($edit_graphic_max_layers)
	formatText TextName = layercount '(%x/%d)' X = <layer_num> d = ($edit_graphic_max_layers)
	add_generic_menu_text_item {
		text = <layercount>
		heading
	}
	if (<layer_num> < ($edit_graphic_max_layers))
		add_generic_menu_text_item {
			text = qs(0x842b9b8a)
			pad_choose_script = edit_graphic_layer_item_select
			pad_choose_params = {my_layer = <layer_num> new}
		}
	endif
	<X> = 0
	<my_text> = qs(0x00000000)
	if (<layer_num> > 0)
		begin
		formatText TextName = my_text '%s' s = ($edit_graphic_layer_infos [<X>].texture_name)
		add_generic_menu_text_item {
			text = <my_text>
			pad_choose_script = edit_graphic_layer_item_select
			pad_choose_params = {my_layer = <X>}
		}
		X = (<X> + 1)
		repeat <layer_num>
	endif
	LaunchEvent \{Type = focus
		target = cagr_edit_vmenu}
	add_user_control_helper \{button = green
		text = qs(0xb870b0e2)}
	add_user_control_helper \{button = red
		text = qs(0xaf4d5dd2)}
endscript

script edit_graphic_exit_main_menu 
	if ($eg_creating_premade)
		edit_graphic_print_layers
	endif
	edit_graphic_prepare_sprite_infos
	generatecagtexture info_array = <sprite_infos> Player = <currentskaterprofileindex> test = 0
	updatecurrentcasmodel \{buildscript = model_apply_created_texture}
	unpauseskaters
	Wait \{5
		gameframes}
	pauseskaters
	if ScreenElementExists \{id = current_menu_anchor}
		DestroyScreenElement \{id = current_menu_anchor}
	endif
	clean_up_user_control_helpers
	edit_graphic_create_pre_menu
endscript
