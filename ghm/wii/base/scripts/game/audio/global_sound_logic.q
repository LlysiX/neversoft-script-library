					qs(0x89688b72)}
				0xec7236b3 Player = <Player> On = true
			else
				printf \{channel = sfx
					qs(0x71c9f9c0)}
				0xec7236b3 Player = <Player> On = true
			endif
		endif
		if (<part> = bass)
			printf \{channel = sfx
				qs(0xf7672ca5)}
			if isSinglePlayerGame
				printf \{channel = sfx
					qs(0x29b80d4d)}
				0xec7236b3 Player = <Player> On = true
			else
				printf \{channel = sfx
					qs(0xa4fbe460)}
				0xec7236b3 Player = <Player> On = true
			endif
		endif
		if (<part> = vocals)
