					0
					0
				]}
			Change \{structurename = player1_status
				save_num_powerups = 0}
			Change \{structurename = player2_status
				save_num_powerups = 0}
			Change \{structurename = player1_status
				save_health = 0.0}
			Change \{structurename = player2_status
				save_health = 0.0}
		endif
	endif
	gamemode_gettype
