script create_multipass_materials \{materials = multipass_materials
		async = 0}
	GetArraySize $<materials>
	material_array_size = <array_Size>
	material_index = 0
	if (<material_array_size> > 0)
		begin
		material_name = ($<materials> [<material_index>].Name)
		GetArraySize ($<materials> [<material_index>].pass)
		pass_index = 0
		begin
		material_pass = ($<materials> [<material_index>].pass [<pass_index>])
		if (<pass_index> = 0)
			material_name_pass = <material_name>
		else
			formatText TextName = pass_text '_pass%p' p = (<pass_index> + 1)
			ExtendCrc <material_name> <pass_text> out = material_name_pass
		endif
		if NOT create_materials_from_list materials = ingame_materials material_to_clone = <material_pass> cloned_material_name = <material_name_pass> async = 0
			create_materials_from_list materials = global_materials material_to_clone = <material_pass> cloned_material_name = <material_name_pass> async = 0
		endif
		if NOT (<pass_index> = 0)
			setmaterialpass Base = <material_name> pass = <material_name_pass>
		endif
		<pass_index> = (<pass_index> + 1)
		repeat <array_Size>
		<material_index> = (<material_index> + 1)
		conditional_async_wait async = <async>
		repeat <material_array_size>
	endif
endscript

script destroy_multipass_materials \{materials = multipass_materials
		async = 0}
	GetArraySize $<materials>
	material_array_size = <array_Size>
	material_index = 0
	if (<material_array_size> > 0)
