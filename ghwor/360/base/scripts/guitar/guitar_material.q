script create_materials_from_list \{async = 0}
	begin
	if creatematerialsfromlist_cfunc <...> async_batch_size = 1 texdicts = ($material_texdicts)
		break
	endif
	if GotParam \{material_to_clone}
		break
	endif
	conditional_async_wait async = <async>
	repeat
endscript

script destroy_materials_from_list 
	GetArraySize $<materials>
	<material_array_size> = <array_Size>
	<index> = 0
	if (<material_array_size> > 0)
		begin
		<material> = ($<materials> [<index>].base_data)
		<props> = ($<materials> [<index>].base_props)
		<list> = ($<materials> [<index>].list)
		GetArraySize <list>
		<list_array_size> = <array_Size>
		<item> = 0
		begin
		<prop> = (<list> [<item>])
		destroymaterial Name = (<prop>.Name)
		<item> = (<item> + 1)
		repeat <list_array_size>
		<index> = (<index> + 1)
		repeat <material_array_size>
	endif
endscript

script create_in_game_materials \{async = 0}
	gem_set = standard_gems
	fx_set = standard_fx
	star_set = standard_stars
	RequireParams \{[
			gem_set
			fx_set
			star_set
		]
		all}
	array = <gem_set>
	GetArraySize $<array>
	index = 0
	begin
	<element> = ($<array> [<index>].element)
	<Color> = ($<array> [<index>].Color)
	SetArrayElement ArrayName = <element> globalarray index = 0 NewValue = (<Color> [0])
	SetArrayElement ArrayName = <element> globalarray index = 1 NewValue = (<Color> [1])
	SetArrayElement ArrayName = <element> globalarray index = 2 NewValue = (<Color> [2])
	SetArrayElement ArrayName = <element> globalarray index = 3 NewValue = (<Color> [3])
	<index> = (<index> + 1)
	repeat <array_Size>
	array = <fx_set>
	GetArraySize $<array>
	index = 0
	begin
	<element> = ($<array> [<index>].element)
	<Color> = ($<array> [<index>].Color)
	SetArrayElement ArrayName = <element> globalarray index = 0 NewValue = (<Color> [0])
	SetArrayElement ArrayName = <element> globalarray index = 1 NewValue = (<Color> [1])
	SetArrayElement ArrayName = <element> globalarray index = 2 NewValue = (<Color> [2])
	SetArrayElement ArrayName = <element> globalarray index = 3 NewValue = (<Color> [3])
	<index> = (<index> + 1)
	repeat <array_Size>
	array = <star_set>
	GetArraySize $<array>
	index = 0
	begin
	<element> = ($<array> [<index>].element)
	<Color> = ($<array> [<index>].Color)
	SetArrayElement ArrayName = <element> globalarray index = 0 NewValue = (<Color> [0])
	SetArrayElement ArrayName = <element> globalarray index = 1 NewValue = (<Color> [1])
	SetArrayElement ArrayName = <element> globalarray index = 2 NewValue = (<Color> [2])
	SetArrayElement ArrayName = <element> globalarray index = 3 NewValue = (<Color> [3])
	<index> = (<index> + 1)
	repeat <array_Size>
	create_materials_from_list materials = ingame_materials async = <async>
endscript

script destroy_in_game_materials 
	destroy_materials_from_list \{materials = ingame_materials}
endscript

script create_global_materials \{async = 0}
	create_materials_from_list materials = global_materials async = <async>
endscript

script destroy_global_materials 
	destroy_materials_from_list \{materials = global_materials}
endscript

