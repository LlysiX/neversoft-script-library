	next_y_scale = <next>
	change_y_scale = <changed>
	update_highway_value dest = <dest_x_pos> curr = <curr_x_pos> max_delta = $delta_pos immediate
	next_x_pos = <next>
	change_x_pos = <changed>
	update_highway_value dest = <dest_extra_offset_factor> curr = <curr_extra_offset_factor> max_delta = $delta_extra_offset_factor immediate
	next_extra_offset_factor = <next>
	change_extra_offset_factor = <changed>
	if (<change_y_scale> = true || <change_x_scale> = true || <change_extra_offset_factor> = true || <force_update> = 1)
		update_highway_scale Player = <Player> x_scale_factor = <next_x_scale> y_scale_factor = <next_y_scale> extra_offset_factor = <next_extra_offset_factor> jamroom_highway = <jamroom_highway>
		<objID> :obj_setmultipletags tags = {curr_x_scale = <next_x_scale> curr_y_scale = <next_y_scale> curr_extra_offset_factor = <next_extra_offset_factor> force_update = 0}
	endif
	if (<change_x_pos> = true)
		GetScreenElementPosition id = <objID>
		next_pos = (((1.0, 0.0) * <next_x_pos>) + ((0.0, 1.0) * (<screenelementpos> [1])))
		<objID> :se_setprops Pos = <next_pos>
		<objID> :obj_setmultipletags tags = {curr_x_pos = <next_x_pos>}
		hud_next_pos = ((1.0, 0.0) * <next_x_pos>)
		pos_highway_hud Player = <Player> Pos = <hud_next_pos>
	endif
	WaitOneGameFrame
	repeat
endscript
