script highway_scale_manager \{jamroom_highway = 0}
	pos_thresh = 15.001
	delta_pos = 15.0
	Obj_GetID
	begin
	<objID> :GetTags
	update_highway_value dest = <dest_x_scale> curr = <curr_x_scale> max_delta = $delta_scale immediate
	next_x_scale = <next>
